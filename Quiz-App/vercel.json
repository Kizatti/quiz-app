{
  "version": 2,
  "framework": "create-react-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true,
        "packageManager": "npm",
        "buildCommand": "npm run build",
        "installCommand": "npm install",
        "env": {
          "NODE_OPTIONS": "--openssl-legacy-provider"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}